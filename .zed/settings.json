{
	"format_on_save": "on",
	"formatter": [{ "code_action": "source.organizeImports" }, "prettier"],
	"languages": {
		"JavaScript": {
			"language_servers": [
				"deno",
				"!typescript-language-server",
				"!vtsls",
				"!eslint",
			],
		},
		"TSX": {
			"language_servers": [
				"deno",
				"!typescript-language-server",
				"!vtsls",
				"!eslint",
			],
		},
		"TypeScript": {
			"language_servers": [
				"deno",
				"!typescript-language-server",
				"!vtsls",
				"!eslint",
			],
		},
	},
	"lsp": { "deno": { "settings": { "deno": { "enable": true } } } },
}
