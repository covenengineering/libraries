{
	"format_on_save": "language_server",
	"code_actions_on_format": {
		"source.organizeImports": true
	},
	"formatter": {
		"code_actions": {
			"source.organizeImports": true
		}
	},
	"lsp": {
		"deno": {
			"settings": {
				"deno": {
					"enable": true
				}
			}
		}
	},
	"languages": {
		"JavaScript": {
			"language_servers": [
				"deno",
				"!typescript-language-server",
				"!vtsls",
				"!eslint"
			]
		},
		"TypeScript": {
			"language_servers": [
				"deno",
				"!typescript-language-server",
				"!vtsls",
				"!eslint"
			]
		},
		"TSX": {
			"language_servers": [
				"deno",
				"!typescript-language-server",
				"!vtsls",
				"!eslint"
			]
		}
	}
}
